  <artifactId>selenium-aes</artifactId>
  <version>0.4</version>
  <packaging>hpi</packaging>
  <name>Selenium Auto Exec Server(AES) plugin</name>
  <description>
    This plugin is a Selenium Auto Exec Server(AES) (http://www.enjoyxstudy.com/selenium/autoexec/) plugin.
  </description>
  <url>http://wiki.hudson-ci.org/display/HUDSON/Selenium+AES+Plugin</url>

  <developers>
    <developer>
      <id>onozaty</id>
    </developer>
  </developers>

  <dependencies>
    <dependency>
      <groupId>com.enjoyxstudy</groupId>
      <artifactId>selenium-autoexec-client</artifactId>
      <version>0.3</version>
      <scope>system</scope>
      <systemPath>${basedir}/src/main/webapp/WEB-INF/lib/selenium-autoexec-client-0.3.jar</systemPath>
    </dependency>
  </dependencies>

  <scm>
    <connection>scm:svn:https://guest@svn.dev.java.net/svn/hudson/tags/selenium-aes-0.4</connection>
    <developerConnection>scm:svn:https://svn.dev.java.net/svn/hudson/tags/selenium-aes-0.4</developerConnection>
    <url>https://hudson.dev.java.net/source/browse/hudson/hudson/plugins/selenium-aes/tags/selenium-aes-0.4</url>
  </scm>
</project>
